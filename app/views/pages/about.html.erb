            <div class="jumbotron" id="landing_page_jumbotron">

              <h1 id="jumbotron_header">TRAVELING ISN'T EASY</h1>
              <p class="lead" id="jumbo_subheadline">Don't do it alone.</p>

              <% if user_signed_in? %>
                <% if current_user.trips.empty? %>
                  <a href="/trips/new"><button id="jumbotron_button" type="button" class="btn btn-secondary btn-lg" data-toggle="modal" data-target=".bs-example-modal-sm">Create Your Trip</button></a>
                <% else %>
                  <a href="/expenses"><button id="jumbotron_button" type="button" class="btn btn-secondary btn-lg" data-toggle="modal" data-target=".bs-example-modal-sm">Enter Dashboard</button></a>
                <% end %>
              <% else %>
                <button id="jumbotron_button" type="button" class="btn btn-secondary btn-lg" data-toggle="modal" data-target=".bs-example-modal-sm">Learn more</button>
                  <div class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
                    <div class="modal-dialog modal-sm">
                      <div class="modal-content" id="jumbotron_dropdown">

                        <h2>Get your TravelPal</h2>
                        <%= form_for(User.new, url: user_registration_path) do |f| %>
                          <%# devise_error_messages! %>


                        <div class="field">
                          <!-- <%= f.label :first_name %><br /> -->
                          <%= f.text_field :first_name, placeholder: "First Name", autofocus: true, :class => 'form-control' %>
                        </div><br/>

                        <div class="field">
                          <!-- <%= f.label :last_name %><br /> -->
                          <%= f.text_field :last_name, placeholder: "Last Name", :class => 'form-control' %>
                        </div><br/>

                      <div class="field">
                        <!-- <%= f.label :email %><br /> -->
                        <%= f.email_field :email, placeholder: "Email", :class => 'form-control' %>
                      </div><br/>

                      <span class="field">
                        <!-- <%= f.label :country %> -->
                        <%= f.select :country, options_for_select([1..100].map { |n| "+" + n.to_s } ), placeholder: "Country Code", :class => 'form-control' %>
                      </span><br/>

                      <span class="field">
                        <!-- <%= f.label :number %> -->
                        <%= f.text_field :number, placeholder: "(+1) Number", :class => 'form-control' %>
                      </span><br/>

                      <div class="field">
                        <!-- <%= f.label :password %> -->
                        <% if @minimum_password_length %>
                        <em>(<%= @minimum_password_length %> characters minimum)</em>
                        <% end %>
                        <%= f.password_field :password, autocomplete: "off", placeholder: "Password", :class => 'form-control' %>
                      </div><br/>

                      <div class="field">
                        <!-- <%= f.label :password_confirmation %> -->
                        <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Password Again", :class => 'form-control' %><br />
                      </div><br/>

                      <div class="actions">
                        <%= f.submit "Sign up", :class => 'btn btn-primary btn-lg' %>
                      </div><br/>
                    <% end %>

                    </div>


                  </div>
                </div>
            <% end %>




