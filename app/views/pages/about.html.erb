                      <%= f.email_field :email, autofocus: true, placeholder: "email" %><br />
                    </div>

                    <div class="field">
                      <%= f.label :number %><br />
                      <%= f.telephone_field :number, placeholder: "number" %><br />
                    </div>

                    <div class="field">
                      <%= f.label :password %>
                      <% if @minimum_password_length %>
                      <em>(<%= @minimum_password_length %> characters minimum)</em>
                      <% end %><br />
                      <%= f.password_field :password, autocomplete: "off", placeholder: "password" %><br />
                    </div>

                    <div class="field">
                      <%= f.label :password_confirmation %><br />
                      <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "re-type password" %><br />
                    </div>

                    <div class="actions"><br />
                      <%= f.submit "Sign up", :class => 'btn btn-primary btn-lg' %> <br/> or log in
                    </div>
                  <% end %>

                </div>
              </div>
